{
  "name": "pointcaster-qt",
  "build": {
    "dockerfile": "../Dockerfile",
    "context": "..",
    "args": {
      "USER_UID": "1001",
      "USER_GID": "1001"
    }
  },
  "remoteUser": "dev",
  "updateRemoteUserUID": false,
  "workspaceFolder": "/pointcaster",
  "workspaceMount": "source=${localWorkspaceFolder},target=/pointcaster,type=bind",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-vscode.cmake-tools",
        "llvm-vs-code-extensions.vscode-clangd",
        "delgan.qml-format"
      ]
    }
  }
}