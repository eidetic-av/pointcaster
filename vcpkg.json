{
  "name": "pointcaster",
  "version-string": "0.0.1",
  "dependencies": [
    {
      "name": "imgui",
      "features": ["docking-experimental"]
    },
    {
      "name": "magnum",
      "features": [
        "debugtools",
        "gl",
        "sdl2application",
        "glxcontext",
        "magnumfont",
        "meshtools",
        "objimporter",
        "primitives",
        "scenegraph",
        "shaders",
        "text",
        "texturetools",
        "trade",
        "tgaimporter",
        "windowlessglxapplication"
      ]
    },
    {
      "name": "magnum-integration",
      "features": ["imgui", "eigen"]
    },
    {
      "name": "opencv4",
      "default-features": false,
      "features": ["contrib", "cuda"]
    },
    {
      "name": "pcl",
      "default-features": false,
      "features":  ["cuda"]
    },
    {
      "name": "sdl2",
      "default-features": false,
      "features": ["x11"],
      "platform": "linux"
    },
    {
      "name": "zeromq",
      "default-features": false,
      "features": ["draft"]
    },
    "azure-kinect-sensor-sdk",
    "bob-pointclouds",
    "concurrentqueue",
    "corrade",
    "cppzmq",
    "cuda",
    "cuda-noise",
    "draco",
    "earcut-hpp",
    "eigen3",
    "fmt",
    "imgui",
    "liblemon",
    "liblo",
    "libremidi",
    "liblemon",
    "libusb",
    "mdns-cpp",
    "msgpack",
    "nanoflann",
    "nativefiledialog",
    "orbbec-sdk",
    "paho-mqttpp3",
    "readerwriterqueue",
    "serdepp",
    "sdl2",
    "spdlog",
    "tbb",
    "toml11",
    "tracy",
    "tweeny",
    "zpp-bits"
  ]
}
