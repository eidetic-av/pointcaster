{
  "name": "bob-pointcaster",
  "version-string": "0.0.1",
  "dependencies": [
    "bob-pointclouds",
    "zpp-bits",
    "earcut-hpp",
    "paho-mqttpp3",
    "msgpack",
    "nlohmann-json",
    "azure-kinect-sensor-sdk",
    "corrade",
    "eigen3",
    "fmt",
    "spdlog",
    "cppzmq",
    "libusb",
    "imgui",
    {
      "name": "opencv",
      "features": ["tbb"],
      "platform": "windows"
    },
    {
      "name": "opencv",
      "platform": "linux"
    },
    {
      "name": "imgui",
      "features": ["docking-experimental"]
    },
    {
      "name": "sdl2",
      "default-features": false,
      "features": ["x11"],
      "platform": "linux"
    },
    {
      "name": "zeromq",
      "default-features": false,
      "features": ["draft"]
    },
    {
      "name": "magnum",
      "features": [
        "debugtools",
        "gl",
        "sdl2application",
        "glxcontext",
        "magnumfont",
        "meshtools",
        "objimporter",
        "primitives",
        "scenegraph",
        "shaders",
        "text",
        "texturetools",
        "trade",
        "tgaimporter",
        "windowlessglxapplication"
      ]
    },
    {
      "name": "magnum-integration",
      "features": ["imgui", "eigen"]
    },
    "sdl2",
    "nativefiledialog"
  ]
}
