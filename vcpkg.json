{
  "name": "bob-pointcaster",
  "version-string": "0.0.1",
  "dependencies": [
    "zpp-bits",
    "spdlog",
    "azure-kinect-sensor-sdk",
    "realsense2",
    "corrade",
    "sdl2",
    "cppzmq",
    "libusb",
    "ixwebsocket",
    "zlib",
    "range-v3",
    "imgui",
    {
      "name": "sdl2",
      "default-features": false,
      "features": [ "x11" ],
      "platform": "linux"
    },
    {
      "name": "zeromq",
      "default-features": false,
      "features": [ "draft" ]
    },
    {
      "name": "magnum",
      "features": [
        "anyimageconverter",
        "anyimageimporter",
        "anysceneconverter",
        "anysceneimporter",
        "anyshaderconverter",
        "debugtools",
        "distancefieldconverter",
        "gl",
        "sdl2application",
        "glxcontext",
        "imageconverter",
        "magnumfont",
        "magnumfontconverter",
        "meshtools",
        "objimporter",
        "primitives",
        "scenegraph",
        "shaderconverter",
        "shaders",
        "shadertools",
        "text",
        "texturetools",
        "trade"
      ]
    },
    {
      "name": "magnum-integration",
      "features": [ "imgui" ]
    }
  ]
}
